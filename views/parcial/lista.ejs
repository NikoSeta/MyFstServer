<br>
<div>
    <h2 style="color:cadetblue"> Lista Productos</h2>
    <% if (aux.length > 0){ %> 
        <table class="table table-dark table-hover">
            <tr><th>Producto</th><th>Precio</th><th>Imágen</th></tr>
            <%for(producto of producto){ %>
             <tr>
                 <td><%=producto.name %>  </td>
                 <td><%=producto.price  %>  </td>
                 <td><%=producto.url  %>  </td>
             </tr>
            <% } %> 
        </table>
    <% } else { %>
        <p class="aler alert-warning">No se encontró productos</p>
    <% } %>
</div>